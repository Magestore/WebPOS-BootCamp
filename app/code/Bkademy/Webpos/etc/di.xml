<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">

    <!-- product api -->
    <preference for="Bkademy\Webpos\Api\Catalog\ProductRepositoryInterface"
                type="Bkademy\Webpos\Model\Catalog\ProductRepository"/>
    <preference for="Bkademy\Webpos\Api\Catalog\SwatchRepositoryInterface"
                type="Bkademy\Webpos\Model\Catalog\SwatchRepository"/>
    <preference for="Bkademy\Webpos\Api\Data\Catalog\SwatchResultInterface"
                type="Magento\Framework\Api\SearchResults"/>
    <preference for="Bkademy\Webpos\Api\Catalog\ProductInterface" type="Bkademy\Webpos\Model\Catalog\Product"/>
    <!-- end product api -->

    <!-- category api -->
    <preference for="Bkademy\Webpos\Api\Catalog\CategoryRepositoryInterface"
                type="Bkademy\Webpos\Model\Catalog\CategoryRepository"/>
    <!-- end category api -->
   
</config>