<page layout='webpos' xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd" >
<body>
    <referenceBlock name="webpos.page">
        <arguments>
            <argument name="jsLayout" xsi:type="array">
                <item name="components" xsi:type="array">
                    <item name="webpos.container" xsi:type="array">
                        <item name="children" xsi:type="array">
                            <item name="checkout" xsi:type="array">
                                <item name="children" xsi:type="array">
                                    <item name="order.list" xsi:type="array">
                                        <item name="component" xsi:type="string">Bkademy_Webpos/js/view/sales/order/list</item>
                                        <item name="displayArea" xsi:type="string">order.list</item>
                                        <item name="children" xsi:type="array">

                                        </item>
                                    </item>
                                </item>
                            </item>
                        </item>
                    </item>
                </item>
            </argument>
        </arguments>
    </referenceBlock>
</body>
</page>

